                                      Type = PARENTHESIS
                                        Type = PARENTHESIS
                                          Type = PARENTHESIS
                                            Type = PARENTHESIS
                                              Type = PARENTHESIS
                                                Type = PARENTHESIS
                                                  Type = PARENTHESIS
                                                    Type = PARENTHESIS
                                                      Type = PARENTHESIS
                                                        Type = PARENTHESIS
                                                          Type = PARENTHESIS
                                                            Type = PARENTHESIS
                                                              Type = PARENTHESIS
                                                                Type = PARENTHESIS
                                                                  Type = PARENTHESIS
                                                                    Type = COMMAND
                                                                    argv: "yes" "(null)" 
                                                                    End COMMAND
                                                                  End PARENTHESIS
                                                                End PARENTHESIS
                                                              End PARENTHESIS
                                                            End PARENTHESIS
                                                          End PARENTHESIS
                                                        End PARENTHESIS
                                                      End PARENTHESIS
                                                    End PARENTHESIS
                                                  End PARENTHESIS
                                                End PARENTHESIS
                                              End PARENTHESIS
                                            End PARENTHESIS
                                          End PARENTHESIS
                                        End PARENTHESIS
                                      End PARENTHESIS
                                    End PARENTHESIS
                                  End PARENTHESIS
                                End PARENTHESIS
                              End PARENTHESIS
                            End PARENTHESIS
                          End PARENTHESIS
                        End PARENTHESIS
