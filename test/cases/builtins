# echo
echo
echo -n
echo -n yes?
echo yes? -n
echo hey
echo hey hey
echo hey  hey
echo hey"ey" hey'ey' "hey"ey hey"ey"
echo "" "" "'
echo -nnnnnnnnnnnnnnn
echo -nnnnnnnnnnnnnnnnnt
# export, unset, env
export
env
export test=changeme
echo $test
export test=changed
echo $test
export
env
unset test
echo $test
export 1=invalid _=valid i1=valid ==invalid i=====valid
env
unset 1 _ i1 = i
env
# cd and pwd
pwd
cd .
pwd
cd "does not exist"
pwd
cd test
pwd
cd ..
pwd
cd /
pwd
cd ..
pwd
# exit (yes this depends on subshells)
(exit 1)
(exit 1 2)
(exit abc)
(exit 1 abc)
(exit 1f)
(exit "     12")
(exit " 22 ")
(exit 9223372036854775807)
